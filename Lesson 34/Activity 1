import java.util.Scanner;

class Lesson_34_Activity_One {
  
   public static int sum(int [] a){
	   int sum = 0;
    for (int i = 0; i < a.length; i++) {
    	sum += a[i];
    }
     return sum;
    }
  
    public static void main(String[] args)
     {
     int list[] = {10,30};
     System.out.println(sum(list));
    }
}
